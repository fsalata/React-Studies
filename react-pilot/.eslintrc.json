{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "no-underscore-dangle": 0,
    "react/require-default-props": 0,
    "jsx-a11y/label-has-for": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "no-plusplus": 0,
    "radix": 0,
    "no-cond-assign": 0,
    "no-param-reassign": 0,
    "react/no-typos": 0,
    "no-new": 0,
    "react/forbid-prop-types": [0],
    "no-undef": [0],
    "no-return-assign": [0]
  }
}
